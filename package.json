{
  "name": "queubole",
  "version": "1.0.0",
  "description": "community communication and assistance application",
  "main": "server.js",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ramonb621/Neighborly.git"
  },
  "author": "Ramon",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ramonb621/Neighborly/issues"
  },
  "homepage": "https://github.com/ramonb621/Neighborly#readme"
}
